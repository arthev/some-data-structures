(defclass pairing-node ()
  ((left
    :accessor left
    :initform nil
    :initarg :left)
   (right
    :accessor right
    :initform nil
    :initarg :right)
   (parent
    :accessor parent
    :initform nil
    :initarg :parent)
   (key
    :accessor key
    :initform nil
    :initarg :key)
   (datum
    :accessor datum
    :initform nil
    :initarg :datum)))

(defclass pairing-heap ()
  ((root
    :accessor root
    :initform nil
    :initarg :root)
   (comp-fn
    :accessor comp-fn
    :initform #'<
    :initarg :comp-fn)))
